        "jade": "1.1.x",
        "dateformat": "2.0.x",
        "querystring": "0.2.x",
        "fabric-ca-client": "1.1.0",
        "fabric-client": "1.1.0",
        "grpc": "1.10.1",
        "cfenv": "1.0.x",
        "lodash": "^4.17.11"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.2",
        "babel-jest": "^22.4.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-minify": "^0.3.0",
        "babel-register": "^6.26.0",
        "chokidar": "^2.0.2",
        "del": "3.0.0",
        "eslint": "^4.18.1",
        "eslint-config-airbnb-base": "^12.0.1",
        "eslint-config-prettier": "^2.6.0",
        "eslint-plugin-flowtype": "^2.46.1",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-prettier": "^2.3.1",
        "flow-bin": "^0.66.0",
        "husky": "^0.14.3",
        "jest": "^22.4.2",
        "jest-cli": "^22.4.2",
        "jest-tobetype": "^1.1.0",
        "lint-staged": "^7.0.0",
        "prettier": "^1.7.1"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>/tools/jest.setup.js",
        "verbose": true,
        "testURL": "http://localhost/",
        "collectCoverage": true,
        "coverageReporters": ["json", "html"]
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add --force"
        ],
        "*.{json}": [
            "prettier --parser json --write",
            "git add --force"
        ]
    },
    "scripts": {
        "precommit": "lint-staged",
        "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
        "fix": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" --fix .",
        "check": "flow check",
        "test": "jest --coverage",
        "test-watch": "jest --watch",
        "build": "node tools/build.js",
        "build-watch": "node tools/build.js --watch",
        "debug": "node --inspect=0.0.0.0:9229  tools/run.js",
        "dev": "node tools/run.js",
        "start": "node build/server.js"
    }
}
